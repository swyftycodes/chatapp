generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") 
}

model signup_attempts {
  id       BigInt @id @default(autoincrement())
  username String
  emailHash    String   @unique
  passwordHash String
  code     Int 
  created  DateTime @default(now())
  sessionid String @unique
}

model users {
  id       BigInt @id @default(autoincrement())
  username String
  emailHash String   @unique
  passwordHash String
  created  DateTime @default(now())
  auth_token String? @unique
}
